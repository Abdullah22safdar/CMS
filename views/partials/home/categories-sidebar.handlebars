<div class="card my-4">
    <h5 class="card-header">Categories</h5>
    <div class="card-body">
        <div class="row">
            <div class="col-lg-6">
                {{#each category}}
                <ul class="list-unstyled mb-0">
                    <li>
                        <a href="#">{{name}}</a>
                    </li>
                </ul>
                {{/each}}
            </div>

        </div>
    </div>
</div>